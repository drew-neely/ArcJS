


%%

define '+' {type: "plus"}
define '*' {type: "times"}
define '(' {type: "open_paren"}
define ')' {type: "close_paren"}
define id  {type: "id"}

%%

Start : E ;

E : E '+' T   {return $1 + $3;}
  | T
  ;

T : T '*' F   {return $1 * $3;}
  | F
  ;

F : '(' E ')' {return $2;      }
  | id        {return $1.value;} 
  ;